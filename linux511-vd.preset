# mkinitcpio preset file for the 'linux511-vd' package

ALL_config="/etc/mkinitcpio.conf"
ALL_kver="/boot/vmlinuz-linux511-vd"

PRESETS=('default')

#default_config="/etc/mkinitcpio.conf"
default_image="/boot/initramfs-linux511-vd.img"
default_options="-z zstd"

#fallback_config="/etc/mkinitcpio.conf"
fallback_image="/boot/initramfs-linux511-vd-fallback.img"
fallback_options="-S autodetect"
